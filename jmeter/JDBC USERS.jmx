          <stringProp name="query">-- original query
WITH origin AS (
SELECT obj_id as main_id, obj_id,object,vertices
FROM sixth.cityobject
WHERE cityjson_id=1 AND type In (&apos;Building&apos;) 
limit 30000),

-- get children of original query
children AS(
SELECT parent_id as main_id, obj_id,object,vertices
FROM  sixth.cityobject
WHERE parent_id IN (SELECT obj_id FROM origin))

(SELECT main_id, obj_id, object,vertices from origin)
UNION ALL (SELECT * from children )
ORDER by main_id</stringProp>
